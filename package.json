{
 "name": "purveyor",
 "version": "1.0.0",
 "description": "",
 "main": "index.js",
 "scripts": {
  "watch-sass": "sass sass/main.scss style/style.css -w",
  "devserver": "live-server",
  "start": "npm-run-all --parallel devserver watch-sass",
  "compile-sass": "sass sass/main.scss style/style.comp.css",
  "concat:css": "concat -o style/style.concat.css style/style.comp.css",
  "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' style/style.concat.css -o style/style.prefix.css ",
  "compress:css": "sass style/style.prefix.css style/style.min.css vendor/normalize.css vendor/animate.css --output-style compressed",
  "build": "npm-run-all compile-sass concat:css prefix:css compress:css"
 },
 "keywords": [],
 "author": "DevArtist",
 "license": "ISC",
 "devDependencies": {
  "autoprefixer": "^9.7.5",
  "concat": "^1.0.3",
  "live-server": "*",
  "sass": "*",
  "npm-run-all": "^4.1.5",
  "postcss-cli": "^7.1.0"
 }
}
